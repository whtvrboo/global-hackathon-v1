# Production Environment Variables
# Copy this file to .env.prod and fill in your actual values

# Database Configuration
DB_USER=folio_user
DB_PASSWORD=your-secure-database-password
DB_NAME=folio_db

# Application Configuration
FRONTEND_URL=https://your-domain.com
JWT_SECRET=your-very-secure-jwt-secret-key

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URL=https://your-domain.com/api/auth/google/callback

# Google Books API
GOOGLE_BOOKS_API_KEY=your-google-books-api-key

# GitHub Repository (for image names)
GITHUB_REPOSITORY=your-username/your-repo-name

# Production Server Configuration (for deployment)
PROD_HOST=your-production-server.com
PROD_USER=deploy
PROD_PORT=22
